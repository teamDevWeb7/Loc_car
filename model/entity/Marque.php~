<?php

namespace Model\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Table(name="marque")
 * @ORM\Entity
 */

class Marque{

    // marque a cmb de vehicule ?, mappedBy -> renseignÃ©e dans l'autre table sous quelle var ?     * @ORM\OneToMany(targetEntity="Vehicule", mappedBy(="marque"))
    /**
     * @ORM\Id
     * @ORM\Column(type="integer", name="id")
     * @ORM\GeneratedValue(strategy="IDENTITY")

     * @var integer
     */
    private int $id;


    /**
     * @ORM\Column(type="string", name="name", length="55")
     * @var string
     */

     private string $name;

    /**
     * @ORM\OneToMany(targetEntity="Vehicule", mappedBy="marque")
     *
     */
     private $vehicules;


    /**
     * Set id.
     *
     * @param int $id
     *
     * @return Marque
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id.
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name.
     *
     * @param string $name
     *
     * @return Marque
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name.
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
